{
  "name": "hr-xmen-hungry-photo",
  "version": "1.1.0",
  "description": "Starter for Express, React, Redux, SCSS applications",
  "scripts": {
    "build-dev": "rimraf dist && webpack --watch  --colors --config webpack.dev.config",
    "dev-server": "nodemon ./server/server.js",
    "dev-database": "mongod --dbpath ./db",
    "client-dev-server": "CLIENT_DEV=true CLIENT_DEV_PORT=3000 webpack-dev-server --watch --colors --config webpack.dev.config --port 3000",
    "build-prod": "rimraf dist && webpack --colors -p --config webpack.prod.config",
    "prod-server": "node ./server/server.js",
    "heroku-postbuild": "npm run build-prod",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack"
  },
  "keywords": [
    "ExpressJS",
    "ReactJS",
    "Redux",
    "React hot loader",
    "React Router",
    "SCSS",
    "Webpack Devevelopment configuration",
    "Webpack Production configuration",
    "Airbnb Eslint",
    "pm2",
    "mocha",
    "chai"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hr-xmen/hr-xmen.git"
  },
  "author": "hr-xmen",
  "license": "MIT",
