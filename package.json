  "description": "Starter for Express, React, Redux, SCSS applications",
  "scripts": {
    "dev": "webpack-dev-server --watch --colors",
    "build": "rimraf dist && NODE_ENV=production webpack --colors",
    "server": "NODE_ENV=production PORT=8080 nodemon ./server/server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "test:watch": "npm run test -- --watch",
    "lint": "eslint src test webpack",
    "database": "mongod --dbpath ./db"
  },
